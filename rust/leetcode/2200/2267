impl Solution {
    pub fn has_valid_path(grid: Vec<Vec<char>>) -> bool {
        let n=grid[0].len();
        let mut f=vec![0;n];
        f[0]=1;
        for s in &grid{
            for (i,&c) in s.iter().enumerate(){
                if i>0 {
                    f[i]|=f[i-1];
                }
                if s[i]=='('{
                    f[i]<<=1;
                }else{
                    f[i]>>=1;
                }

            }
        }
        f[n-1]&1u128>0
    }
}